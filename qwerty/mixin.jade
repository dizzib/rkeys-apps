mixin qwerty-r
  .qwerty-r.layout-verticaly
    .row-0
      +keys(6, 7, 8, 9, 0, '-', '=', 'backslash \\', '`')
    .row-1
      +keys('y', 'u', 'i', 'o', 'p', '[', ']', 'BackSpace Bksp')
    .row-2
      +keys('h', 'j', 'k', 'l', ';', '\'', 'Return')
    .row-3
      +keys('n', 'm', ',', '.', '/')
      +key('Shift_R Shift').w-3
    .row-4
      +key('space', ' ').w-4
      +keys('Delete Del', 'Super_R Super', 'Alt_R Alt', 'Control_R Ctrl')
