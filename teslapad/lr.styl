@require './base'

.qwerty-l
.qwerty-r
  position    relative
  z-index     150
  #f.up, #j.up
    background-color red
  .lane-4
    .key:nth-child(5n)
      opacity     0.5
  .thumb
    opacity     0.5

.root
  height      6 * K
  overflow    hidden

.sidepad
  background-color rgba(0, 0, 0, 0.5)
  height      5 * K
  overflow    auto
  position    fixed
  width       3 * K
  z-index     200
  .scrollpad
    width       10 * K
